{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/recognize/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/recognize",
      "methods": ["GET", "POST"],
      "dest": "/api/recognize/index.js"
    }
  ]
}