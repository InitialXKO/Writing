{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(?:[^/]+/)*[^/]+\\.(?:js|css|woff2)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/_next/static/$&"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}